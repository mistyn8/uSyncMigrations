{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "uSync":  "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "Umbraco": {
    "CMS": {
      "Unattended": {
        //"InstallUnattended": true,
        //"PackageMigrationsUnattended": true,
        //"UpgradeUnattended": true,
        //"UnattendedUserName": "admin@example.com",
        //"UnattendedUserEmail": "admin@example.com",
        //"UnattendedUserPassword": "1234567890"
      },
      "Content": {
        "MacroErrors": "Throw"
      },
      "Hosting": {
        "Debug": true
      },
      "RuntimeMinification": {
        "UseInMemoryCache": true,
        "CacheBuster": "Timestamp"
      }
    }
  },
  "uSync": {
    "Settings": {
      "ImportAtStartup": "None",
      "ExportAtStartup": "None",
      "ExportOnSave": "All",
      "UiEnabledGroups": "All"
    },
    "Migrations": {
      "GridIgnoreIsFullWidth": true
    }
  }
}
